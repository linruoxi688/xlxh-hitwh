  white-space: nowrap;
}
.text-unit {
  margin-top: 24rpx;
  color: rgb(0, 0, 0);
  font-size: 40rpx;
  line-height: 29rpx;
  white-space: nowrap;
}
.button-light {
  padding: 45rpx 0 41rpx;
  color: rgb(10, 10, 51);
  font-size: 40rpx;
  font-weight: 700;
  line-height: 38rpx;
  white-space: nowrap;
  border-radius: 12rpx;
  border: solid 2rpx rgb(224, 226, 235);
}
.button-primary {
  margin-top: 32rpx;
  padding: 45rpx 0;
  color: rgb(255, 255, 255);
  font-size: 40rpx;
