.image-pause {
  width: 48rpx;
  height: 48rpx;
}
.page {
  padding: 238rpx 50rpx 0;
  background-color: rgb(248, 250, 255);
  width: 100%;
  overflow-y: auto;
  height: 100%;
}
.guide {
  align-self: center;
  color: rgb(10, 10, 51);
  font-size: 72rpx;
  line-height: 67rpx;
  white-space: nowrap;
}
.timer {
  margin-top: 170rpx;
  padding: 126rpx 0 105rpx;
  align-self: center;
  border-radius: 50%;
  background-image: url('https://codefun-proj-user-res-1256085488.cos.ap-guangzhou.myqcloud.com/6245823a5a7e3f0310b8a7f3/62458267468f3700116dd059/16487343656687549301.png');
